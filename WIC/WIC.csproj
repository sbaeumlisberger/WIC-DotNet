<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net8.0-windows</TargetFrameworks>
		<LangVersion>12.0</LangVersion>
		<Nullable>enable</Nullable>
		<Authors>sbaeumlisberger</Authors>
		<Company></Company>
		<Copyright>© 2024 sbaeumlisberger</Copyright>
		<PackageProjectUrl>https://github.com/sbaeumlisberger/WIC-DotNet</PackageProjectUrl>
		<RepositoryUrl>https://github.com/sbaeumlisberger/WIC-DotNet</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageTags>windows imaging image-processing windows-imaging-component wic com-interop bitmap docoding encoding</PackageTags>
		<Product>WIC</Product>
		<Description>A .NET library that makes the Windows Imaging Component (WIC) available to managed code. </Description>
		<Version>2.0.0-beta1</Version>
		<PackageId>WIC.DotNet</PackageId>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedAllSources>true</EmbedAllSources>
		<Deterministic>true</Deterministic>
		<PathMap>$(MSBuildProjectDirectory)=C:\</PathMap>
		<Platforms>AnyCPU;x64</Platforms>
		<IsAotCompatible>true</IsAotCompatible>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>
	<ItemGroup>
		<None Include="README.md" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>
